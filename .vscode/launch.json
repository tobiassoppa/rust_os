{
  "configurations": [
    {
      "name": "Remote debug",
      "type": "gdb",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "target": "${workspaceRoot}/target/aarch64-unknown-none-softfloat/debug/kernel", 
      "gdbpath" : "arm-none-eabi-gdb",
      "autorun": [
          "source -v debug.gdb",
      ]
  }
  ]
}